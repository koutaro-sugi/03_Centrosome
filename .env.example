# OpenWeatherMap API
REACT_APP_OPENWEATHERMAP_API_KEY=your_api_key_here

# AWS Configuration (Optional - Amplify manages most of these)
REACT_APP_AWS_REGION=ap-northeast-1
# IoT endpoint: prefer REACT_APP_IOT_ENDPOINT; falls back to REACT_APP_AWS_IOT_ENDPOINT
# Example: a12ai23qgl4xhl-ats.iot.ap-northeast-1.amazonaws.com
REACT_APP_IOT_ENDPOINT=
REACT_APP_AWS_IOT_ENDPOINT=

# Mado Sensor Configuration
REACT_APP_MADO_DEVICE_ID=M-X
REACT_APP_MADO_TOPIC_PREFIX=mado/centra

# Application Settings
REACT_APP_VERSION=0.1.0
REACT_APP_ENV=development

# WebSocket Configuration (for MAVLink)
REACT_APP_WEBSOCKET_URL=ws://localhost:8080

# Map Settings (if using maps)
REACT_APP_MAP_DEFAULT_LAT=35.6762
REACT_APP_MAP_DEFAULT_LON=139.6503
REACT_APP_MAP_DEFAULT_ZOOM=10

# Feature Flags
REACT_APP_ENABLE_WEATHER_FORECAST=true
REACT_APP_ENABLE_MADO_INTEGRATION=true
REACT_APP_ENABLE_FLIGHT_PLANNING=true
